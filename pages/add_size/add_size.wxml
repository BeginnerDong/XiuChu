	<view class="ilblock color1 font26">胸围<text class="color3 font20">cm</text></view>
	<input class="ilblock color2 font22" placeholder="请输入胸围" style="margin-left: 50rpx;position: relative; top: 17rpx;"
	value="{{web_mainData.bust}}"  data-key="bust" bindblur="inputChange"/>
</view>
<view class="list bg1">
	<view class="ilblock color1 font26">腰围<text class="color3 font20">cm</text></view>
	<input class="ilblock color2 font22" placeholder="请输入腰围" style="margin-left: 50rpx;position: relative; top: 17rpx;"
	value="{{web_mainData.waistline}}"  data-key="waistline" bindblur="inputChange"/>
</view>
