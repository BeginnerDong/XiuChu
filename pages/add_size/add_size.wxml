<view class="list bg1">
	<view class="ilblock color1 font26">身高<text class="color3 font20">cm</text></view>
	<input class="ilblock color2 font22" placeholder="请输入身高" style="margin-left: 50rpx;position: relative; top: 17rpx;" 
	value="{{web_mainData.height}}"  data-key="height" bindblur="inputChange"/>
</view>
<view class="list bg1">
	<view class="ilblock color1 font26">体重<text class="color3 font20">kg</text></view>
	<input class="ilblock color2 font22" placeholder="请输入体重" style="margin-left: 50rpx;position: relative; top: 17rpx;"
	value="{{web_mainData.weight}}"  data-key="weight" bindblur="inputChange"/>
</view>
