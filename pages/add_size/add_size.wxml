<view class="list bg1">
	<view class="ilblock color1 font26">设为默认尺寸</view>
	<view class="body-view" style="display: inline-block;float: right;">
		<switch bindchange="switch2Change" checked="{{web_isdefault==1?true:''}}"/>
	</view>
</view>

<button class="bg3 radiu15 font24 color5" type="button" bindtap="{{web_buttonCanClick?'submit':''}}">确认</button>